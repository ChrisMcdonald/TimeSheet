function disable_input() {
    $("#edit_time_sheet_<%= @time_sheet.id %>").find(':input[type="number"]').each(function (i) {
        if (i > 1) {
            $(this).prop('disabled', true)
            console.log(true)
        }
        console.log(i)
    })

}
