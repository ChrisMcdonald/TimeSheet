<div class="container"  >
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @project.name %>
</p>
<p>




<div class="col-sm-12 message-form">
  <%= form_for(@project ,
        html: {class: 'horizontal-form'}, remote: true) do |f| %>

    <div class="col-sm-5">
      <div class="form-group">


        <div class="field">
          <%= f.label :name %>
          <%= f.text_field :name , class: 'form-control' %>
        </div>

         <div class="actions">
          <%= f.submit %>
        </div>
      </div>
    </div>

      <%end%>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>


</div>